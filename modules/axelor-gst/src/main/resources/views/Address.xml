<?xml version="1.0" encoding="UTF-8"?>
<object-views xmlns="http://axelor.com/xml/ns/object-views"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://axelor.com/xml/ns/object-views
  http://axelor.com/xml/ns/object-views/object-views_5.0.xsd">

			
	<grid name="address-grid" title="Address"
		model="com.axelor.gst.db.Address">

		<field name="type" />
		<field name="lineOne"/>
        <field name="lineTwo"/>
        <field name="pincode" />
        
	</grid>

	<form name="address-form" title="Address"
		model="com.axelor.gst.db.Address" css="panel-red">

		<panel title="Address">
			<field name="type" />
		<field name="lineOne"/>
        <field name="lineTwo"/>
        <field name="city" required="true"/>
        <field name="state" />
        <field name="country" required="true"/>
        <field name="pincode" />
		</panel>

	</form>

    
</object-views>